<li><a href="<?php echo route('damusGetDetailPublic', $public->id); ?>"><?php echo $public->title; ?></a></li>
<?php if( count ( $public->children ) > 0 ): ?>
	<ul>
		<?php foreach( $public->children as $public ): ?> 
			<?php echo $__env->make('admin.damus.partials.public', $public, array_except(get_defined_vars(), array('__data', '__path')))->render(); ?>
		<?php endforeach; ?>
	</ul>
<?php endif; ?>
